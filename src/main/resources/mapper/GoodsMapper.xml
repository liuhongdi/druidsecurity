<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.druidsecurity.demo.mapper.GoodsMapper">

	<select id="selectOneGoods" parameterType="long" resultType="com.druidsecurity.demo.pojo.Goods">
        select * from goods where goodsId=#{goodsId}
    </select>

</mapper>